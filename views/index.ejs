                       aria-expanded="false" aria-controls="collapseReward">показать</a>
                </h2>

                <div class="collapse" id="collapseReward">
                    <div class="reward_list">
                        <% rewards.forEach(function(item){ %>
                        <article class="reward_item">
                            <img alt="" src="img/reward/<%= item.img_src || 'default.png' %>" width="70" height="100">

                            <h3>
                                <i class="fa fa-angle-double-left"></i>
                                <%= item.title %>
                                <i class="fa fa-angle-double-right"></i>
                            </h3>
                        </article>
                        <% }); %>
                    </div>
                </div>
            </section>
        </div>
    </section>

    <section class="gray" id="dates">
        <div class="container text-center" id="collapseDates">
            <h1>Даты</h1>
            <hr class="subtitle-divider"/>

            <ol class="text-left">
                <% var dates_i = 0; dates.forEach(function (item) { %>
                <li>
                    <% dates_i++; %>
                    <%= item.title %>
                    <a class="pull-right" role="button" data-toggle="collapse" href="#collapseDate_<%= dates_i; %>"
                       aria-expanded="false" aria-controls="collapseDate_<%= dates_i; %>">показать</a>
                    <ul class="collapse text-left" id="collapseDate_<%= dates_i++ %>">
                        <% item.list.forEach(function (li){ %>
                        <li><%= li %></li>
                        <% }) %>
                    </ul>
                </li>
                <% }) %>
            </ol>
        </div>
    </section>

    <section class="white">
        <div class="container text-center">
            <h1 class="text-uppercase text-center">Виртуальный тур по паруснику</h1>
            <hr class="subtitle-divider"/>
            <button id="toggleVirtualShip" class="btn btn-default-lg" role="button">Осмотреть парусник</button>
        </div>
        <iframe src="about:blank" style="display:none;" frameborder="0" width="100%" height="400" id="virtualShip"
